# Environment variables for Nexus Eternal
# Copy this file to .env and modify as needed

# Development settings
NODE_ENV=development
VITE_DEBUG_MODE=true
VITE_SHOW_STATS=true

# Audio settings
VITE_MASTER_VOLUME=1.0
VITE_MUSIC_VOLUME=0.7
VITE_SFX_VOLUME=0.8

# Game settings
VITE_ENABLE_CHEATS=true
VITE_AUTO_SAVE=true
VITE_SAVE_INTERVAL=30000

# Graphics settings
VITE_DEFAULT_QUALITY=high
VITE_ENABLE_SHADOWS=true
VITE_ENABLE_POST_PROCESSING=true
VITE_TARGET_FPS=60

# API endpoints (if needed for leaderboards, etc.)
# VITE_API_BASE_URL=https://api.nexuseternal.com
# VITE_API_KEY=your_api_key_here